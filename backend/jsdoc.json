{
  "source": {
    "include": [
      "src/",
      "src/constant",
      "src/controllers",
      "src/errors",
      "src/middleware",
      "src/routes/",
      "src/routes/api",
      "src/routes/utils",
      "src/service/",
      "src/service/interface",
      "src/utils"
    ],
    "exclude": [
      "node_modules"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs"
  }
}